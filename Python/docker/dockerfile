FROM ubuntu:20.04

LABEL mad162pumpkin="dmjon162@yandex.ru"

RUN apt-get update && \
    apt-get install -y python && \
    apt-get install -y git && \
    rm -rf /var/lib/apt/lists/* &&\
    apt-get clean

ARG CACHEBUST=1

RUN git clone https://github.com/mad162pumpkin/Python_D.git &&\
    apt-get clean

WORKDIR /Python_D

RUN python test.py
