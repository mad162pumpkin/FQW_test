FROM ubuntu:20.04

LABEL mad162pumpkin="dmjon162@yandex.ru"

RUN apt-get update && \
    apt-get install -y default-jre && \
    apt-get install -y default-jdk && \
    apt-get install -y git && \
    apt-get clean

CMD git clone https://github.com/mad162pumpkin/Java_D.git

RUN java Java_D/test.java
RUN test
