FROM ubuntu:20.04

LABEL mad162pumpkin="dmjon162@yandex.ru"

RUN apt-get update && \
    apt-get install -y default-jre && \
    apt-get install -y default-jdk && \
    apt-get install -y git && \
    apt-get clean

ARG NOCACHE

RUN --no-cache git clone https://github.com/mad162pumpkin/Java_D.git &&\
    --no-cache apt-get clean

WORKDIR /Java_D

RUN --no-cache java test.java
